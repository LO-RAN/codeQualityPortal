  <Rule TypeName="ExampleRule" Category="Compuware.Structure" CheckId="SMP0000">
    <Name>Namespaces should begin with the company name 'FooBar'</Name>
    <Description>
      All Namespaces should be named {Company}.{Application}.{Feature}.{SubFeature}
      eg. FooBar.Calulator.UI
    </Description>
    <GroupOwner>FooBar</GroupOwner>
    <DevOwner>Paul Glavich</DevOwner>
    <Owner>Paul Glavich</Owner>
    <Url>http://localhost</Url>
    <Resolution>Alter the namespace '{0}' so that 'FooBar' is the first component.</Resolution>
    <Email>someone@foobar.com.au</Email>
    <MessageLevel Certainty="100">Error</MessageLevel>
    <FixCategories>Breaking</FixCategories>
	</Rule>
